function name: printThisText
//input: ThisText
//output: none
//what it does: prints the input String to Serial 

void setup(){
  Serial.begin(9600);
}

void loop() {
  int ThisText;
Serial.println(ThisText);
delay(500);
}


function name: multiplyIntegerByTwo
//input: a number
//output: a number
//what it does: multiplies the input number by 2 and returns the result
void setup(){
  Serial.begin(9600);
}

void loop() {
  int j;
  int k;

  k = multiplyIntegerByTwo(2*j); 
  Serial.println(k);
  delay(500);
}

int multiplyIntegerByTwo(int y){
  int result;
  result = 2 * y;
  return result;
}


function name: multiplyDecimalByTwo
//input: a number
//output: a number
//what it does: multiplies the input number by 3.14159 and returns the result
void setup(){
  Serial.begin(9600);
}

void loop() {
  int p = 3.14159;
  int j;
  int r;
  
  r = multiplyDecimalByTwo(p, j); 
  Serial.println(k);
  delay(500);
}

int multiplyDecimalByTwo(int x, int y){
  int result;
  result = x * y;
  return result;
}
